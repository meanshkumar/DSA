#include<iostream>
using namespace std;
int main()
{
char ch;
cin>>ch;
if(ch>=65&&ch<=90)
{
    cout<<"This is an uppercase alphabet"<<" " <<ch<<endl;
}
else if(ch>=97&&ch<=122)
{
    cout<<"This is an lowercase alphabet"<<ch<<endl;
}
if(ch>=48&&ch<=57)
{
    cout<<"This is an natural number"<<ch<<endl;
}
return 0;
}
